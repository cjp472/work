}
.but_fix{
  bottom: 0;
  width: 100%;
  height: 110rpx;
  color: #fff;
  background-color: #f6f6f6;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0 140rpx;
}
.but_fix view{
  width: 200rpx;
  border-radius: 12rpx;
    height:80rpx;
}