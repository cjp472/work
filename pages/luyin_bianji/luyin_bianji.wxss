page {
  background-color: #f6f6f6;
  border-top: 1rpx solid #cdcdcd;
  padding-bottom: 20rpx;
}

.content {
  width: 700rpx;
  height: 990rpx;
  background-color: #fff;
  margin: 20rpx auto 0;
  border-radius: 12rpx;
  padding: 25rpx 30rpx;
  box-sizing: border-box;
}

.confirm {
  width: 710rpx;
  margin: 70rpx auto 0;
  height: 100rpx;
  color: #fff;
  border-radius: 12rpx;
  letter-spacing: 12rpx;
}

.text_box {
  height: 230rpx;
  width: 640rpx;
  background-color: #fafafa;
  border: 1rpx solid #cbcbcb;
  box-sizing: border-box;
  padding: 15rpx 10rpx;

}
.text_css{
    box-sizing: border-box;
      height: 210rpx;
}
.title_box_b{
  padding-bottom: 5rpx;
box-sizing: border-box;
border-bottom: 1rpx solid #cbcbcb;
}
.title_box{
   width: 610rpx;
  margin: 0 auto 30rpx;
}


.content_but {
  width: 190rpx;
  height: 75rpx;
  border-radius: 12rpx;
  color: #fff;
  margin: 0 auto;
}

.img_box {
  width: 640rpx;
  height: 420rpx;
  background-color: #fafafa;
  border: 2rpx dotted #b7b7b7;
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
}

.img_box_i {
  width: 640rpx;
  height: 420rpx;

}
.img_box_img{
  width: 640rpx;
  height: 420rpx;
}
.textarea{
  height: 200rpx;
}
.Close{
  height:60rpx;
  width:58rpx;
  position: absolute;
  top: 0;
