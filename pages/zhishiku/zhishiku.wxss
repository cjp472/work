/* pages/zhishiku/zhishiku.wxss */

page {
  background-color: #f6f6f6;
  border-top: 1rpx solid #cdcdcd;
  padding-top: 20rpx;
}

.confirm {
  width: 710rpx;
  margin: 30rpx auto 0;
  height: 100rpx;
  color: #fff;
  border-radius: 12rpx;
  letter-spacing: 12rpx;
}

.name {
  width: 710rpx;
  margin: 0 auto 25rpx;
  background-color: #fff;
  border-radius: 12rpx;
  padding: 20rpx 35rpx;
  box-sizing: border-box;
  font-weight: 500;
  color: #333;
}

.name_b {
  padding-bottom: 5rpx;
  margin-left: 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #cbcbcb;
}

.name_right {
  width: 295rpx;
  height: 50rpx;
  box-sizing: border-box;
  background-color: #fafafa;
  border: 1rpx solid #d7d7d7;
  border-radius: 12rpx;
  color: #666;
  position: relative;
  text-align: center;
  line-height: 50rpx;
}

.name_right_Popup {
  position: absolute;
  width: 295rpx;
  height: 100rpx;
  background-color: #fafafa;
  border: 1rpx solid #d7d7d7;
  top: 53rpx;
  border-radius: 12rpx;

}
.but{
  width: 190rpx;
  height: 75rpx;
  color: #fff;
     border-radius: 12rpx;
}
.img_box {
  width: 650rpx;
  height: 588rpx;
  background-color: #fafafa;
  border: 2rpx dotted #b7b7b7;
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
}

.img_box_i {
  width: 640rpx;
  height: 588rpx;
position: relative;
}
.img_box_img{
 width: 640rpx;
  height: 588rpx;
}
.close_icon2{
  position: absolute;
  right: 0;
  top: 0;
  width: 29rpx;
  height: 30rpx;
}
.Close{
  height:60rpx;
  width:58rpx;
  position: absolute;
  top: 0;
  right: 0;

}
